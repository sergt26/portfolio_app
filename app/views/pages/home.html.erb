<h1>Hi, <%= current_user.first_name %></h1>
<p>Find me in app/views/pages/home.html.erb</p>

<h1>Blog Posts</h1>

<% @posts.each do |post| %>
  <p><%= post.title %></p>
  <p><%= post.body %></p>
<% end %>

<h1>Skills</h1>

<% @skills.each do |skill| %>
  <p><%= skill.title %></p>
  <p><%= skill.percent_utilized %></p>
  <p><%= skill.badge %></p>
<% end %>
